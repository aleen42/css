{
    "structure": {
        "readme": "README.md"
    },
    "styles": {
        "website": "style/website.css",
        "ebook": "style/ebook.css",
        "pdf": "style/pdf.css",
        "mobi": "style/mobi.css",
        "epub": "style/epub.css"
    },
    "plugins": [
        "disqus",
        "-search",
        "github",
        "sharing",
        "tbfed-pagefooter",
        "ga"
    ],
    "pluginsConfig": {
        "disqus": {
            "shortName": "personal"
        },
        "search-pro": {
            "cutWordLib": "nodejieba",
            "defineWord": [
                "Gitbook Use"
            ]
        },
        "github": {
            "url": "https://github.com/aleen42"
        },
        "sharing": {
            "weibo": true,
            "facebook": true,
            "twitter": true,
            "google": true,
            "instapaper": false,
            "vk": false,
            "all": [
                "facebook", "google", "twitter",
                    "weibo", "instapaper"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright Â© aleen42",
            "modify_label": "Updated time: ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "ga": {
            "token": "UA-75457732-1"
        }
    }
}